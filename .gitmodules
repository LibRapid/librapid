[submodule "librapid/vendor/jitify"] # Jitify module for CUDA JIT compilation
	path = librapid/vendor/jitify
	url = https://github.com/Pencilcaseman/jitify.git
[submodule "librapid/vendor/Vc"] # Vc module for SIMD instructions
	path = librapid/vendor/Vc
	url = https://github.com/VcDevel/Vc.git
[submodule "librapid/vendor/fmt"] # {fmt} module for fast formatting and IO
	path = librapid/vendor/fmt
	url = https://github.com/fmtlib/fmt.git
[submodule "librapid/vendor/scnlib"] # {scnlib} module for fast reverse formatting and IO
	path = librapid/vendor/scnlib
	url = https://github.com/eliaskosunen/scnlib.git
[submodule "librapid/vendor/mpfr"] # Big number support with MPFR and MPIR
	path = librapid/vendor/mpfr
	url = https://github.com/pencilcaseman/mpfr
[submodule "librapid/vendor/nanobench"]
	path = librapid/vendor/nanobench
	url = https://github.com/martinus/nanobench.git
[submodule "librapid/vendor/Catch2"]
	path = librapid/vendor/Catch2
	url = https://github.com/catchorg/Catch2.git
