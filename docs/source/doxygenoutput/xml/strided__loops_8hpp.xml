			coord[idim] = 0; \
			a_i = a_i - (_extent[idim] - 1) * _stride_a[idim]; \
		} \
		else \
		{ \
			a_i = a_i + _stride_a[idim]; \
			break; \
		} \
	} \
} while (idim &lt; ndim); \
delete[] coord;</initializer>
