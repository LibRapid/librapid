        <name>LIBRAPID_SIMPLE_IO_IMPL</name>
        <param><defname>TEMPLATE_</defname></param>
        <param><defname>TYPE_</defname></param>
        <initializer>	template&lt;TEMPLATE_&gt;                                                                            \
	struct fmt::formatter&lt;TYPE_&gt; {                                                                 \
		std::string formatStr = &quot;{}&quot;;                                                              \
                                                                                                   \
		template&lt;typename ParseContext&gt;                                                            \
		constexpr auto parse(ParseContext &amp;ctx) {                                                  \
			formatStr = &quot;{:&quot;;                                                                      \
			auto it	  = ctx.begin();                                                               \
			for (; it != ctx.end(); ++it) {                                                        \
				if (*it == &apos;}&apos;) break;                                                             \
				formatStr += *it;                                                                  \
			}                                                                                      \
			formatStr += &quot;}&quot;;                                                                      \
			return it;                                                                             \
		}                                                                                          \
                                                                                                   \
		template&lt;typename FormatContext&gt;                                                           \
		auto format(const TYPE_ &amp;object, FormatContext &amp;ctx) {                                     \
			try {                                                                                  \
				return fmt::format_to(ctx.out(), object.str());                                    \
			} catch (std::exception &amp; e) { return fmt::format_to(ctx.out(), e.what()); }           \
		}                                                                                          \
	};                                                                                             \
                                                                                                   \
	template&lt;TEMPLATE_&gt;                                                                            \
	std::ostream &amp;operator&lt;&lt;(std::ostream &amp;os, const TYPE_ &amp;object) {                              \
		os &lt;&lt; object.str();                                                                        \
		return os;                                                                                 \
	}</initializer>
